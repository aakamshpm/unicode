{
  "name": "unicode",
  "version": "1.0.0",
  "private": true,
  "description": "Unicode coding platform",
  "scripts": {
    "docker:up": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up -d postgres redis rabbitmq",
    "docker:down": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml down",
    "docker:prod": "docker-compose -f docker-compose.yml up -d --build",
    "docker:prod:down": "docker-compose -f docker-compose.yml down",
    "docker:prod:logs": "docker-compose -f docker-compose.yml logs -f",
    "docker:logs": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml logs -f",
    "backend": "npm run start:dev --workspace=backend",
    "frontend": "npm run dev --workspace=frontend",
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "dev:full": "npm run docker:up && concurrently \"npm run backend\" \"npm run frontend\"",
    "install-all": "npm install && npm install --workspace=backend && npm install --workspace=frontend",
    "seed:roles": "npm run seed:roles --workspace=backend",
    "create:admin": "npm run create:admin --workspace=backend"
  },
  "workspaces": [
    "backend",
    "frontend"
  ],
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "unicode",
    "coding-platform",
    "nestjs",
    "react"
  ],
  "author": "aakamshpm",
  "license": "MIT"
}
